[
    {
        "type": "Auxiliary",
        "name": "profit",
        "attributes": {
            "formula": "(price * harvest) - capital"
        }
    },
    {
        "type": "Auxiliary",
        "name": "growth_goal",
        "attributes": {
            "formula": "capital * 0.1"
        }
    },
    {
        "type": "Auxiliary",
        "name": "capital_lifetime",
        "attributes": {
            "formula": "20"
        }
    },
    {
        "type": "Stock",
        "name": "resource",
        "attributes": {
            "formula": "1000"
        }
    },
    {
        "type": "Flow",
        "name": "regeneration",
        "attributes": {
            "formula": "resource * regeneration_rate"
        },
    },
    { "type": "Fills",     "from": "regeneration", "to": "resource" },
    {
        "type": "Flow",
        "name": "harvest",
        "attributes": {
            "formula": "capital * yield"
        },
    },
    { "type": "Drains",    "from": "resource",     "to": "harvest" },
    {
        "type": "Stock",
        "name": "capital",
        "attributes": {
            "formula": "5"
        }
    },
    {
        "type": "Flow",
        "name": "depreciation",
        "attributes": {
            "formula": "capital / capital_lifetime"
        }
    },
    { "type": "Drains",    "from": "capital",     "to": "depreciation" },

    {
        "type": "Flow",
        "name": "investment",
        "attributes": {
            "formula": "min(profit, growth_goal)"
